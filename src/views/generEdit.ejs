<% include partials/_header %>

<div class="container">
  <div class="row">
    <!--FORM-->
    <div class="col-md-5 offset-md-3">
      <div class="card">
        <div class="card-body">
          <form action="/gener/edit/<%= gener._id %>" method="post">
            <div class="form-group">
              <input class="form-control" type="text" name="name" placeholder="Nombre" value="<%= gener.name %>">
            </div>
            <div class="form-group">
                <input class="form-control" type="text" name="url" placeholder="url" value="<%= gener.url %>">
            </div>
            
            <button class="btn btn-primary" type="submit">
              Update 
            </button>
          </form>
        </div>
      </div>
    </div>
  </div>
</div>

<% include partials/_footer%>